apiVersion: 'v1'
kind: 'PersistentVolumeClaim'
metadata:
  # ClaimRef used by PV
  name: '<%= nameclaimrefmanifest %>'
  namespace: '<%= namespace %>'
spec:
  # Name of PV
  volumeName: '<%= namemanifest %>'
  accessModes:
    - '<%= volume.access %>'
  resources:
    requests:
      storage: '<%= volume.capacity %>'
---