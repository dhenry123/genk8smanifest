apiVersion: 'v1'
kind: 'Service'
metadata:
  name: 'glusterfs-cluster'
  namespace: '<%= namespace %>'
spec:
  ports:
  - port: 1
---
apiVersion: 'v1'
kind: 'Endpoints'
metadata:
  name: 'glusterfs-cluster'
  namespace: '<%= namespace %>'
subsets:
- addresses:
  - ip: '<%= glusterfsipaddr %>'
  ports:
  - port: 1
---